{
    "extends": "google",
    "rules": {
        "semi": 0,
        "no-trailing-spaces": 0,
        "quotes": 0,
        "spaced-comment": 0,
        "comma-dangle": 0,
        "key-spacing": 0,
        "no-multi-spaces": 0,
        "max-len": 0,
        "arrow-parens": 0,
        "no-unused-vars": 0,
        "padded-blocks": 0,
        "eol-last": 0,
        "space-before-function-paren": 0,
        "camelcase": 0,
        "brace-style": 0,
        "require-jsdoc": 0,
        "no-multiple-empty-lines": 0,
        "space-before-blocks": 0,
        "object-curly-spacing": 0,
        "no-var": 0,
        "guard-for-in": 0,
        "no-throw-literal": 0
    },
     "env": { "es6": true }     
}
